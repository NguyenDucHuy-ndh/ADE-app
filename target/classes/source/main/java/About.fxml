<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<AnchorPane prefHeight="720.0" prefWidth="720.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.About">
   <children>
      <ImageView fitHeight="720.0" fitWidth="720.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Image/blue%20-%20About.jpg" />
         </image>
      </ImageView>
      <Button layoutX="43.0" layoutY="53.0" mnemonicParsing="false" onAction="#edit" prefHeight="50.0" prefWidth="160.0" stylesheets="@style.css" text="Edit">
         <graphic>
            <ImageView fitHeight="57.0" fitWidth="48.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@Image/edit.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button layoutX="277.0" layoutY="53.0" mnemonicParsing="false" onAction="#add" prefHeight="50.0" prefWidth="160.0" stylesheets="@style.css" text="Add">
         <graphic>
            <ImageView fitHeight="57.0" fitWidth="48.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@Image/add.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button layoutX="514.0" layoutY="53.0" mnemonicParsing="false" onAction="#delete" prefHeight="50.0" prefWidth="160.0" stylesheets="@style.css" text="Delete">
         <graphic>
            <ImageView fitHeight="57.0" fitWidth="48.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@Image/delete.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <BorderPane fx:id="loadAbout" layoutY="177.0" prefHeight="565.0" prefWidth="720.0" />
   </children>
</AnchorPane>
